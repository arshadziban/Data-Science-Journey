    "from typing import List\n",
    "\n",
    "vector = List[float]\n",
    "\n",
    "def scalar_multiply(scalar: float, v: vector) -> vector:\n",
    "    return [scalar * v_i for v_i in v]\n",
    "\n",
    "# Example Usage:\n",
    "print(scalar_multiply(2, [1, 2, 3])) \n"
   ]
