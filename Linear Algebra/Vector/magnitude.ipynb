   "source": [
    "import math\n",
    "from typing import List\n",
    "\n",
    "def sum_of_squares(v: List[float]) -> float:\n",
    "    return sum(x**2 for x in v)\n",
    "\n",
    "def magnitude(v: List[float]) -> float:\n",
    "    return math.sqrt(sum_of_squares(v))\n",
    "\n",
    "vector = [3, 4]\n",
    "print(\"Magnitude:\", magnitude(vector)) "
   ]
